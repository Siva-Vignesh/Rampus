<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f2ca37"
    tools:context=".thirdyear_bsccs">
    <ImageView
        android:layout_width="1000dp"
        android:layout_height="250dp"
        android:background="@drawable/whitecircle"
        android:layout_marginTop="180dp"
        android:layout_marginStart="-300dp"
        android:layout_marginEnd="10dp"
        android:rotation="-45"/>
    <TextView
        android:id="@+id/bsccs_s5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="25dp"
        android:layout_marginTop="75dp"
        android:fontFamily="@font/opensansbold"
        android:text="Fifth Semester"
        android:textColor="#111111"
        android:textSize="25sp" />

    <Button
        android:id="@+id/bsccs_advancejava"
        android:layout_width="150dp"
        android:layout_height="75dp"
        android:layout_below="@+id/bsccs_s5"
        android:layout_alignParentStart="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="13dp"
        android:layout_marginEnd="20dp"
        android:layout_toStartOf="@+id/bsccs_ost"
        android:background="@drawable/gradient1"
        android:fontFamily="@font/opensansbold"
        android:text="Advanced Java \nProgramming"
        android:textColor="#ffffff"
        android:textSize="10sp" />

    <Button
        android:id="@+id/bsccs_ost"
        android:layout_width="150dp"
        android:layout_height="75dp"
        android:layout_below="@+id/bsccs_s5"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="20dp"
        android:background="@drawable/gradient2"
        android:fontFamily="@font/opensansbold"
        android:text="Open Source Technology"
        android:textColor="#ffffff"
        android:textSize="10sp"/>

    <Button
        android:id="@+id/bsccs_dcn"
        android:layout_width="150dp"
        android:layout_height="75dp"
        android:layout_below="@+id/bsccs_advancejava"
        android:layout_alignParentStart="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="20dp"
        android:layout_toStartOf="@+id/bsccs_dm"
        android:background="@drawable/gradient5"
        android:fontFamily="@font/opensansbold"
        android:text="Data Communication and Network "
        android:textColor="#ffffff"
        android:textSize="10sp" />

    <Button
        android:id="@+id/bsccs_dm"
        android:layout_width="150dp"
        android:layout_height="75dp"
        android:layout_below="@+id/bsccs_ost"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="20dp"
        android:background="@drawable/gradient3"
        android:fontFamily="@font/opensansbold"
        android:text="Data Mining"
        android:textColor="#ffffff"
        android:textSize="10sp" />

    <Button
        android:id="@+id/bsccs_cd"
        android:layout_width="150dp"
        android:layout_height="75dp"
        android:layout_below="@+id/bsccs_dcn"
        android:layout_alignParentStart="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="20dp"
        android:layout_toStartOf="@+id/bsccs_sa"
        android:background="@drawable/gradient3"
        android:fontFamily="@font/opensansbold"
        android:text="Compilier Design "
        android:textColor="#ffffff"
        android:textSize="10sp" />

    <Button
        android:id="@+id/bsccs_sa"
        android:layout_width="150dp"
        android:layout_height="75dp"
        android:layout_below="@+id/bsccs_dcn"
        android:layout_alignStart="@+id/bsccs_dm"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="20dp"
        android:background="@drawable/gradient4"
        android:fontFamily="@font/opensansbold"
        android:text="Software Agent"
        android:textColor="#ffffff"
        android:textSize="10sp"/>
    <Button
        android:id="@+id/bsccs_evs"
        android:layout_width="300dp"
        android:layout_height="75dp"
        android:layout_below="@+id/bsccs_cd"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="20dp"
        android:background="@drawable/gradient5"
        android:text="Environmental Studies"
        android:textColor="#ffffff"
        android:textSize="10sp" />

    <com.google.android.gms.ads.AdView
        xmlns:ads="http://schemas.android.com/apk/res-auto"
        android:id="@+id/adView125"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_alignParentBottom="true"
        ads:adSize="BANNER"
        ads:adUnitId="ca-app-pub-9384783085331384/5899237284">
    </com.google.android.gms.ads.AdView>


</RelativeLayout>